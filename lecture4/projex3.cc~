#include <iostream>
using std::cout;
using std::endl;
using std::cin;


int main (){
  cout << "Type a  number:"  << endl;
  int i;
  cin >> i;
  int var;
  int counter = 0;  
  for (int var=2; var<i-1; var++){   
    if (i%var == 0){
	cout << "Your number is not prime. It is divisble by: " << var << endl;
	counter++;}}

  if (counter == 0){
       cout << "Your number is prime"  << endl;
  }}


//C-x C-w : write under new name

